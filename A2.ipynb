   "source": [
    "def scalar_product(list1, list2):\n",
    "    total_product = 0\n",
    "    for i in range(len(list1)):\n",
    "        total_product = total_product + (list1[i] * list2[i])\n",
    "\n",
    "    return total_product\n"
   ]
