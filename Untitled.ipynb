    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "\n",
    "N = 10000000\n",
    "x_i = np.random.random(size = N)\n",
    "y_i = np.random.random(size = N)\n",
    "is_inside = x_i**2 + y_i**2 < 1\n",
    "\n",
    "ax.scatter(x_i[is_inside], y_i[is_inside])\n",
    "print(4*np.sum(is_inside)/N)"
   ]
